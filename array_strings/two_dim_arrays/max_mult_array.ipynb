    "def max_num(a):\n",
    "    max_num=0\n",
    "    for sub in a:\n",
    "        for item in sub:\n",
    "            if item > max_num:\n",
    "                print(\"max_num\",max_num)\n",
    "                print(\"item\",item)\n",
    "                max_num=item\n",
    "    return max_num\n",
    "a=[1,2,3],[4,50,6]\n",
    "print(max_num(a))"
