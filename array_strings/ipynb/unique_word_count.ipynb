    "def uniq(ls):\n",
    "    words=ls.split()\n",
    "    dic={}\n",
    "    for w in words:\n",
    "        if w in dic:\n",
    "            continue\n",
    "        else:\n",
    "            dic[w]=ls.count(w)\n",
    "    return dic\n",
    "print(uniq(\"my my name prathap\"))"
   ]
  }
