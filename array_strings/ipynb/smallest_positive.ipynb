   "source": [
    "# smallest positive number\n",
    "def sm_num(ls):\n",
    "    sm=ls[0]\n",
    "    for i in ls:\n",
    "        if i >=0:\n",
    "            sm=min(sm,i)\n",
    "    return sm\n",
    "\n",
    "print(sm_num([1,-2,2,10-20]))      "
   ]
