    "def small_p(ls):\n",
    "    sm_pos=0\n",
    "    for num in ls:\n",
    "        if num >0:\n",
    "            if num < sm_pos:\n",
    "                sm_pos=num\n",
    "    return sm_pos\n",
    "        \n",
    "print(small_p([1,-2,2,10-20]))       "
   ]
