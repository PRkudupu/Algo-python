   ],
   "source": [
    "def unique_list(ls1,ls2):\n",
    "    stack=ls1.copy()\n",
    "    for n2 in ls2:\n",
    "        if n2 not in ls1:\n",
    "            stack.append(n2)\n",
    "    return stack\n",
    "#copy is used to create a list \n",
    "print(unique_list([1,2,3],[3,4,5]))"
   ]
  },
